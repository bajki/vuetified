// Import Fonts Font-awesome and Material Icons
@import url('https://fonts.googleapis.com/css?family=Raleway:300,400,600')
@import url('https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
@import url('https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons')
// Import Vuetify Colors
// Import Vuetify Main styling
@import '../../../node_modules/vuetify/src/stylus/main'
// All Custom Style In Your App
[v-cloak] 
    display: none
#main-app,
#main-app .container
    min-height: 700px
    overflow: hidden
    z-index: 0
#main-app .input-group__details:after
    background-color: rgba(255, 255, 255, 0.32) !important
#main-app .input-group--focused .input-group__append-icon
    color: inherit !important
.fade-transition
  transition: opacity 0.2s ease
.fade-enter, .fade-leave
  opacity: 0
.main--text
  color: #607D8B